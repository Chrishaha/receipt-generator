{
  "name": "receipt-generator",
  "version": "1.0.0",
  "description": "An easy to use online tool to generate receipts.",
  "scripts": {
    "start": "node api/index.js",
    "dev": "parcel index.html",
    "watch": "parcel index.html",
    "build": "rm -rf .parcel-cache && rm -rf dist && parcel build index.html --public-url=./ --no-source-maps",
    "deploy": "npm run build && vercel",
    "dp": "npm run build && vercel && vercel --prod"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vijayhardaha/receipt-generator.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vijayhardaha/receipt-generator/issues"
  },
  "homepage": "https://github.com/vijayhardaha/receipt-generator#readme",
  "dependencies": {
    "chrome-aws-lambda": "^10.1.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "file-saver": "^2.0.5",
    "inputmask": "^5.0.6",
    "jquery": "^3.6.0",
    "puppeteer-core": "^10.4.0",
    "puppeteer": "^10.4.0",
    "tippy.js": "^6.3.2"
  },
  "devDependencies": {
    "@parcel/transformer-sass": "^2.0.0",
    "parcel": "^2.0.0",
    "sass": "^1.43.2"
  }
}
