		"bootstrap": "^5.1.3",
		"bootstrap-material-datetimepicker": "^2.7.3",
		"file-saver": "^2.0.5",
		"inputmask": "^5.0.8",
		"jquery": "^3.7.1",
		"moment": "^2.29.4",
		"parcel": "^2.10.1",
		"postcss": "^8.4.31",
		"posthtml-include": "^1.7.4",
		"sass": "^1.69.5"
	}
