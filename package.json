{
	"name": "receipt-generator",
	"version": "1.1.0",
	"description": "An easy to use online tool to generate receipts.",
	"scripts": {
		"server": "NODE_ENV=development node server.js",
		"dev": "parcel src/index.html --port 3001",
		"build": "parcel build src/index.html --no-source-maps --public-url https://receipt-generator.vercel.app"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/vijayhardaha/receipt-generator.git"
	},
