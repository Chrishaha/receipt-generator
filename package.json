	"license": "MIT",
	"bugs": {
		"url": "https://github.com/vijayhardaha/receipt-generator/issues"
	},
	"homepage": "https://github.com/vijayhardaha/receipt-generator#readme",
	"browserslist": [
		">= 0.5%",
		"last 2 major versions",
		"Chrome >= 60",
		"Firefox >= 60",
		"Firefox ESR",
		"iOS >= 12",
		"Safari >= 12",
		"not Explorer <= 11"
	],
	"dependencies": {
